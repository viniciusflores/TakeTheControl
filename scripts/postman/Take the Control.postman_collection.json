				],
				"body": {
					"mode": "raw",
					"raw": "{\n  \"name\":\"{{username}}\",\n  \"email\":\"{{username}}{{$timestamp}}@email.com\",\n  \"password\":\"{{password}}\"\n}"
				},
				"url": {
					"raw": "{{host}}/users",
